{
    "name": "@app/search-api-e2e",
    "version": "0.0.1",
    "private": true,
    "nx": {
        "implicitDependencies": [
            "@app/search-api"
        ],
        "targets": {
            "e2e": {
                "executor": "@nx/jest:jest",
                "outputs": [
                    "{projectRoot}/test-output/jest/coverage"
                ],
                "options": {
                    "jestConfig": "apps/search-api-e2e/jest.config.cts",
                    "passWithNoTests": true
                },
                "dependsOn": [
                    "@app/search-api:build",
                    "@app/search-api:serve"
                ]
            }
        }
    }
}
