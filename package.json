        "pg": "^8.16.3",
        "pino": "^10.1.0",
        "pino-http": "^11.0.0",
        "postgraphile": "^4.14.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.13.0",
        "uuid": "^13.0.0"
    },
