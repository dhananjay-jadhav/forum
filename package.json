{
    "name": "@app/source",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "e2e": "nx e2e",
        "lint": "nx run-many --target=lint --all --parallel",
        "all:test": "nx run-many --target=test --all --parallel --passWithNoTests",
        "all:build": "nx run-many --target=build --all --parallel",
        "format": "nx format:write",
        "prepare": "husky",
        "codegen": "graphql-codegen --config libs/gql/codegen.ts",
        "codegen:watch": "graphql-codegen --config libs/gql/codegen.ts --watch",
        "db:up": "docker compose up -d --wait",
        "db:down": "docker compose down -v",
        "db:logs": "docker compose logs -f",
        "db:migrate": "node scripts/db-migrate.js migrate",
        "db:watch": "node scripts/db-migrate.js watch",
        "db:commit": "node scripts/db-migrate.js commit",
        "db:uncommit": "node scripts/db-migrate.js uncommit",
        "db:reset": "node scripts/db-migrate.js reset --erase",
        "db:status": "node scripts/db-migrate.js status",
        "gen:app": "node scripts/generate-app.js",
        "gen:lib": "node scripts/generate-lib.js",
        "gen:gql-crud": "node scripts/generate-gql-crud.js",
        "perf:test": "ts-node --project performance/tsconfig.json performance/src/cli.ts",
        "perf:list": "ts-node --project performance/tsconfig.json performance/src/cli.ts --list",
        "perf:api": "ts-node --project performance/tsconfig.json performance/src/cli.ts --service=api",
        "perf:analytics": "ts-node --project performance/tsconfig.json performance/src/cli.ts --service=analytics",
        "perf:search": "ts-node --project performance/tsconfig.json performance/src/cli.ts --service=search",
        "perf:rest": "ts-node --project performance/tsconfig.json performance/src/cli.ts --category=rest",
        "perf:graphql": "ts-node --project performance/tsconfig.json performance/src/cli.ts --category=graphql",
        "perf:run": "ts-node --project performance/tsconfig.json performance/src/cli.ts --endpoint",
        "perf:stress": "CONNECTIONS=100 DURATION=60 ts-node --project performance/tsconfig.json performance/src/cli.ts"
    },
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.11.0",
        "@elastic/elasticsearch": "^9.2.0",
        "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
